<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AudreySpotifyApp.Views.ArtistPage">
    <ContentPage.Content>
        <StackLayout>
            <Image Source="{Binding ArtistImage}" />
            <Label Text="{Binding ArtistName}" />
            <Label Text="{Binding ArtistGenres}" />
            <Label Text="{Binding ArtistFollowers}" />
            <Label Text="{Binding ArtistPopularity}" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>